    "prod": "npm run build && NODE_ENV=production node ./dist/index.js",
    "swagger-document": "NODE_ENV=local ts-node ./swagger.ts",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts,.js --max-warnings=0",
